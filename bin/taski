#!/bin/sh

### dmenu -y 41 -x 3 -p 'Run ' -h 23 | sh
set -- zdocs mdocs msearch

printf '%s\n' "$@" | dmenu -p 'Run ' -h 23 | {
   while read apps; do
      case $apps in
	 zdocs) printf '%s\n' "zdocs -z" ;;
	 mdocs) printf '%s\n' "zdocs -m" ;;
	 *)     printf '%s' "$apps"      ;;
      esac
   done
} | sh

